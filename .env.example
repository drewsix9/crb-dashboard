# Supabase Configuration
# Copy this file to .env.local and fill in your values
# DO NOT commit .env.local to version control

# Supabase Project URL
VITE_SUPABASE_URL=your-project-url-here

# Supabase Anon/Public Key
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Enable mock data mode during development
# Set to 'true' to use mock data instead of Supabase
VITE_USE_MOCK_DATA=true

# Optional: FastAPI Backend URL (for image uploads)
VITE_FASTAPI_URL=http://your-fastapi-server:8000

# Optional: Realtime update interval (milliseconds)
# Default: 30000 (30 seconds)
VITE_REALTIME_INTERVAL=30000

# Optional: Enable debug mode
VITE_DEBUG_MODE=false
